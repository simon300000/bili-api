  "scripts": {
    "test": "npm run unit",
    "unit": "nyc mocha --reporter=landing",
    "lint": "eslint --fix .",
    "coverage": "mkdir coverage; nyc report --reporter=text-lcov >> ./coverage/lcov.info",
